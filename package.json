{
  "name": "tscircuit-deploy",
  "private": true,
  "description": "Vercel-like deployment platform for tscircuit projects",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "build": "bun run db:generate; cd app; bun x astro build",
    "build:action": "cd github-action && bun tsup",
    "start": "cd app && bun x astro dev",
    "test": "bun test",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md,yml,yaml}\"",
    "check": "tsc --noEmit",
    "clean": "rm -rf node_modules dist",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:studio": "prisma studio",
    "db:reset": "prisma migrate reset"
  },
  "dependencies": {
    "@actions/core": "^1.11.1",
    "@actions/github": "^6.0.0",
    "@astrojs/react": "^4.3.0",
    "@astrojs/vercel": "8.2.3",
    "@octokit/types": "^14.1.0",
    "@prisma/client": "^6.12.0",
    "@tailwindcss/vite": "^4.1.11",
    "@tscircuit/runframe": "^0.0.733",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "astro": "5.12.3",
    "circuit-json-to-simple-3d": "^0.0.4",
    "circuit-to-svg": "^0.0.166",
    "dotenv": "^17.2.0",
    "hono": "^4.6.13",
    "ky": "^1.8.1",
    "lucide-react": "^0.525.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "sharp": "^0.34.3",
    "tailwindcss": "^4.1.11",
    "tar": "^7.4.3",
    "tscircuit": "^0.0.534",
    "tsup": "^8.5.0",
    "ulid": "^3.0.1",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@types/bun": "^1.2.18",
    "@types/node": "^20.11.0",
    "prettier": "^3.6.0",
    "prisma": "^6.12.0",
    "typescript": "^5.8.3"
  },
  "engines": {
    "bun": ">=1.2.15"
  },
  "packageManager": "bun@1.2.15",
  "repository": {
    "type": "git",
    "url": "https://github.com/tscircuit/tscircuit-deploy.git"
  },
  "license": "MIT"
}
