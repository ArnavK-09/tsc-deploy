{
  "name": "@tscircuit-deploy/github-action",
  "version": "1.0.0",
  "description": "GitHub Action for tscircuit Deploy",
  "private": true,
  "main": "dist/index.js",
  "scripts": {
    "build": "ncc build src/index.ts -o dist --source-map",
    "dev": "bun run --hot src/index.ts",
    "check-types": "tsc --noEmit",
    "package": "npm run build"
  },
  "dependencies": {
    "@actions/core": "^1.11.1",
    "@actions/exec": "^1.1.1",
    "@actions/github": "^6.0.1",
    "@octokit/rest": "^21.1.1",
    "@tscircuit/eval": "^0.0.248",
    "@tscircuit-deploy/shared": "workspace:*",
    "ky": "^1.8.1",
    "octokit": "^5.0.3",
    "tscircuit": "^0.0.522",
    "ulid": "^3.0.1",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@types/node": "^20.19.6",
    "@vercel/ncc": "^0.38.3",
    "tsup": "^8.5.0",
    "typescript": "^5.8.3"
  }
}
