{
  "name": "@tscircuit-deploy/github-action",
  "version": "1.0.0",
  "description": "GitHub Action for tscircuit Deploy",
  "type": "module",
  "private": true,
  "main": "dist/index.js",
  "scripts": {
    "build": "ncc build src/index.ts -o dist --source-map",
    "dev": "bun run --hot src/index.ts",
    "check-types": "tsc --noEmit",
    "package": "npm run build"
  },
  "dependencies": {
    "@actions/core": "^1.10.1",
    "@actions/exec": "^1.1.1",
    "@actions/github": "^6.0.0",
    "@actions/tool-cache": "^2.0.1",
    "@octokit/rest": "^21.0.1",
    "@tscircuit/cli": "^0.1.158",
    "@tscircuit-deploy/shared": "workspace:*",
    "circuit-json-to-simple-3d": "^0.0.4",
    "circuit-to-svg": "^0.0.166",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "@types/minimatch": "^6.0.0",
    "@vercel/ncc": "^0.38.1",
    "typescript": "^5.8.2"
  },
  "keywords": [
    "github-action",
    "tscircuit",
    "deploy",
    "electronics",
    "pcb"
  ]
}