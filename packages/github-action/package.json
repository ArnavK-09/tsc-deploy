{
  "name": "@tscircuit-deploy/github-action",
  "version": "1.0.0",
  "description": "GitHub Action for tscircuit Deploy",
  "type": "module",
  "private": true,
  "main": "dist/index.js",
  "scripts": {
    "build": "ncc build src/index.ts -o dist --source-map",
    "dev": "bun run --hot src/index.ts",
    "check-types": "tsc --noEmit",
    "package": "npm run build"
  },
  "dependencies": {
    "@actions/core": "^1.11.1",
    "@actions/exec": "^1.1.1",
    "@actions/github": "^6.0.1",
    "@actions/tool-cache": "^2.0.2",
    "@octokit/rest": "^21.1.1",
    "@tscircuit/eval": "^0.0.248",
    "@tscircuit/simple-3d-svg": "^0.0.22",
    "circuit-json-to-simple-3d": "latest",
    "circuit-to-svg": "^0.0.86",
    "tscircuit": "^0.0.522",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@types/node": "^20.19.6",
    "@types/minimatch": "^5.1.2",
    "@vercel/ncc": "^0.38.3",
    "typescript": "^5.8.3"
  },
  "keywords": [
    "github-action",
    "tscircuit",
    "deploy",
    "electronics",
    "pcb"
  ]
}